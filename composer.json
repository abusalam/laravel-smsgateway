{
    "name": "abusalam/laravel-smsgateway",
    "description": "Laravel Implementation of SMS Gateway",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "php": ">=7.1",
        "illuminate/support": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Abu Salam Parvez Alam",
            "email": "abusalamparvezalam@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AbuSalam\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbuSalam\\SmsGatewayServiceProvider"
            ],
            "aliases": {
                "SmsGateway": "AbuSalam\\SmsGatewayFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
